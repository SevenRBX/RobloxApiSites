@model Roblox.Administration.Partials.BreadcrumbsModel

<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="/">Home</a></li>
        @foreach (var item in Model.pageSections.GetRange(0, Model.pageSections.Count - 1))
        {
            <li class="breadcrumb-item"><a href="@item.url">@item.name</a></li>
        }
        <li class="breadcrumb-item active">@Model.currentPage.name</li>
    </ol> 
</nav>