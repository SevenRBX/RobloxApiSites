{
    "version": "2",
    "checks": {
        "method-lines": {
            "config": {
                "threshold": 50
            }
        },
        "similar-code": {
            "config": {
                "threshold": 75
            }
        }
    },
    "exclude_patterns": [
        "**/*.UnitTest/*",
        "**/*.IntegrationTest/*",
        "**/Program.cs",
        "**/Startup.cs",
        "**/Services/Roblox.Services/Migrations/sql/*",
        "**/InternalSites/Roblox.Administration/wwwroot"
    ]
}